<%- include("partials/header")%>
<h1>Home</h1>
<p><%= paragraph %></p>
<% posts.forEach(function(post) { %>
    <% let title = post.title; %>
    <% let content = post.content; %>
    <h1><%= title %></h1>
    <p>
        <%= content.substring(0 , 100) + "..." %>
        <a href= "/posts/<%= title %>" >Read more</a>
    </p>
    
<%});%>
<%- include("partials/footer")%> 